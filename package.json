{
  "name": "eslint.io",
  "version": "0.0.0",
  "description": "Browser version of ESLint",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "npm run browserify-eslint",
    "browserify-eslint": "shx rm -rf eslint && git clone https://github.com/eslint/eslint eslint && cd eslint && npm install && npm run browserify",
    "develop": "NODE_ENV=development webpack-dev-server --port 8014 --no-info",
    "bundle": "NODE_ENV=production webpack --display-error-details",
    "check-updates": "ncu",
    "upgrade": "npm run check-updates -- --upgradeAll && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/circlecell/eslint.io.git"
  },
  "keywords": [
    "eslint",
    "jslint",
    "lint"
  ],
  "author": "Circlecell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/circlecell/eslint.io/issues"
  },
  "homepage": "https://github.com/circlecell/eslint.io#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "copy-webpack-plugin": "^3.0.1",
    "eslint": "^3.6.1",
    "eslint-config-canonical": "^1.8.8",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "npm-check-updates": "^2.8.0",
    "open-browser-webpack-plugin": "0.0.2",
    "shelljs": "^0.7.4",
    "shx": "^0.1.4",
    "string-replace-loader": "^1.0.5",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "babel-eslint": "^7.0.0",
    "babel-polyfill": "^6.13.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsdoc": "^2.3.1",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0"
  }
}
