{
  "name": "eslint.io",
  "private": true,
  "description": "A browser version of ESLint",
  "main": "index.js",
  "scripts": {
    "test": "node test",
    "install-everything": "npm install && npm run build-eslint",
    "build-eslint": "rimraf eslint && npm run clone-eslint && npm run webpackify-eslint && cd eslint && npm install --production",
    "clone-eslint": "git clone https://github.com/eslint/eslint.git eslint",
    "webpackify-eslint": "node tools/webpackify-eslint",
    "develop": "npm run bundle-configs && NODE_ENV=development webpack-dev-server --port 8014 --no-info",
    "bundle": "npm run bundle-configs && NODE_ENV=production webpack --display-error-details",
    "bundle-configs": "mkdirp configs && node tools/bundle-configs",
    "check-updates": "ncu",
    "upgrade": "npm run check-updates -- --upgradeAll && npm install",
    "semantic-release": "semantic-release pre && npm run deploy && semantic-release post",
    "deploy": "pm2 deploy ecosystem.json production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/circlecell/eslint.io.git"
  },
  "keywords": [
    "eslint",
    "jslint",
    "lint"
  ],
  "author": "Circlecell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/circlecell/eslint.io/issues"
  },
  "homepage": "https://github.com/circlecell/eslint.io#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "copy-webpack-plugin": "^3.0.1",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-canonical": "^1.8.8",
    "eslint-config-eslint": "^3.0.0",
    "eslint-config-google": "^0.6.0",
    "eslint-config-standard": "^6.2.0",
    "html-webpack-plugin": "^2.22.0",
    "jasmine": "^2.5.2",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "npm-check-updates": "^2.8.0",
    "open-browser-webpack-plugin": "0.0.2",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.4",
    "shx": "^0.1.4",
    "string-replace-loader": "^1.0.5",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "semantic-release": "^4.3.5"
  },
  "dependencies": {
    "babel-eslint": "^7.0.0",
    "babel-polyfill": "^6.13.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "matreshka": "^2.0.0-alpha.13"
  }
}
