{
  "name": "eslint.io",
  "version": "0.0.0-auto",
  "private": true,
  "description": "A browser version of ESLint",
  "main": "index.js",
  "scripts": {
    "test": "node test && npm run lint || echo 'Errored'",
    "install-all": "npm -v && npm i && lerna exec --concurrency 1 -- npm i",
    "build-eslint": "rimraf eslint && npm run clone-eslint && npm run webpackify-eslint && cd eslint && npm install --production",
    "clone-eslint": "git clone https://github.com/eslint/eslint.git eslint",
    "webpackify-eslint": "node tools/webpackify-eslint",
    "develop": "NODE_ENV=development PORT=8014 concurrently --kill-others 'nodemon ./packages/backend --watch packages/backend' 'webpack-dev-server --no-info --config webpack'",
    "build": "NODE_ENV=production webpack --config webpack --display-error-details",
    "upgrade": "ncu -u -a -x eslint-plugin-jsx-a11y && npm install && lerna exec --concurrency 1 -- ncu -a && lerna exec -- npm install",
    "semantic-release": "semantic-release pre && ssh-keyscan 192.241.157.86 >> ~/.ssh/known_hosts && npm run deploy && semantic-release post",
    "deploy": "pm2 deploy ecosystem.json production --force",
    "travis-deploy": "if-env TRAVIS_BRANCH=master TRAVIS_PULL_REQUEST=false && npm run semantic-release || echo 'Deploy is not run'",
    "lint": "eslint .",
    "open-browser": "opn http://localhost:$PORT",
    "try-production": "NODE_ENV=production PORT=8016 concurrently 'npm run open-browser' 'node ./packages/backend'"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "release": {
    "getLastRelease": "last-release-git"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/circlecell/eslint.io.git"
  },
  "keywords": [
    "eslint",
    "jslint",
    "lint"
  ],
  "author": "Circlecell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/circlecell/eslint.io/issues"
  },
  "homepage": "https://github.com/circlecell/eslint.io#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "espree": "^3.4.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "ghooks": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "if-env": "^1.0.0",
    "last-release-git": "0.0.3",
    "lerna": "^2.0.0-rc.5",
    "nodemon": "^1.11.0",
    "npm": "^5.0.3",
    "npm-check-updates": "^2.11.3",
    "open-browser-webpack-plugin": "0.0.5",
    "opn-cli": "^3.1.0",
    "postcss-calc": "^6.0.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.0.2",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.6",
    "semver": "^5.3.0",
    "style-loader": "^0.18.2",
    "validate-commit-msg": "^2.12.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
